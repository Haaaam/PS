from collections import deque
import sys
input=sys.stdin.readline
queue=deque()
for i in range(int(input()):
    array=input().split()
    if array[0]=='push_front'
        queue.insert(0,array[1])
    elif array[0]=='push_back':
        queue.append(array[1])
    elif array[0]=='pop_front':
        if queue:
            print(queue[0])
            qeueue.popleft()
         elif not queue:
             print(-1)
         elif array[0]=='pop_back':
             if queue:
                 print(queue[-1])
                 queue.pop()
             elif not queue:
                 print(-1)
         if array[0]=='empty':
            if queue:
                print(0)
            else:
                print(1)
        if array[0]=='size':
            print(len(queue))
        if array[0]=='front':
            if queue:
                print(queue[0])
            else:
                print(-1)
        if array[0]=='back':
            if queue:
                print(queue[-1])
            else:
                print(-1)
        
